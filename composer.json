{
  "name": "swoft-fork/event",
  "type": "library",
  "version": "v2.0.11",
  "keywords": [
    "php",
    "swoole",
    "psr-14",
    "swoft",
    "event",
    "event-dispatcher"
  ],
  "description": "swoft event dispatcher component",
  "homepage": "https://github.com/sky-up/swoft-event",
  "license": "Apache-2.0",
  "require": {
    "php": ">8.0",
    "swoft-fork/stdlib": "~2.0.0"
  },
  "autoload": {
    "psr-4": {
      "Swoft\\Event\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SwoftTest\\Event\\Unit\\": "test/unit/",
      "SwoftTest\\Event\\Testing\\": "test/testing/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5 || ^8.0"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  }
}
